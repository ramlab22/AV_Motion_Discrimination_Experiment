function [Results_MLE] = MLE_Calculations_A_V_AV(x,AUD_p,VIS_p,AV_p,AUD_mdl, VIS_mdl, AV_mdl,AUD_pc,VIS_pc,AV_pc,AUD_yData,VIS_yData, AV_yData)
% get residuals of all models
AUD_fittedValues = feval(AUD_mdl, x);
AUD_residuals = AUD_yData - AUD_fittedValues;
VIS_fittedValues = feval(VIS_mdl, x);
VIS_residuals = VIS_yData - VIS_fittedValues;
AV_fittedValues = feval(AV_mdl, x);
AV_residuals = AV_y - AV_fittedValues;

%get measured AUD and VIS
Results_MLE.AUD_R2 = 1-sum(AUD_residuals.^2)/sum((AUD_pc-mean(AUD_pc)).^2); % R2 = 1 – SSresid / SStotal
Results_MLE.VIS_R2 = 1-sum(VIS_residuals.^2)/sum((VIS_pc-mean(VIS_pc)).^2); % R2 = 1 – SSresid / SStotal
Results_MLE.AUD_Mu = AUD_mdl.Coefficients{1, 1};
Results_MLE.AUD_SD = AUD_mdl.Coefficients{1, 2};
Results_MLE.AUD_Variance = Results_MLE.AUD_SD^2;
Results_MLE.VIS_Mu = VIS_mdl.Coefficients{1, 1};
Results_MLE.VIS_SD = VIS_mdl.Coefficients{1, 2};
Results_MLE.VIS_Variance = Results_MLE.VIS_SD^2;

Results_MLE.AUD_functionx = x;
Results_MLE.AUD_functiony = AUD_p;
Results_MLE.VIS_functionx = x;
Results_MLE.VIS_functiony = VIS_p;

%get measured AV function parameters
Results_MLE.AV_Mu = AV_mdl.Coefficients{1, 1};
Results_MLE.AV_SD = AV_mdl.Coefficients{1, 2};
Results_MLE.AV_Variance = Results_MLE.AV_SD^2;
Results_MLE.AV_R2 = 1-sum(AV_residuals.^2)/sum((AV_pc-mean(AV_pc)).^2); % R2 = 1 – SSresid / SStotal
Results_MLE.AV_functionx = x;
Results_MLE.AV_functiony = AV_p;

%Get MLE results
Results_MLE.AUD_Westimate= (1/Results_MLE.AUD_Variance)/((1/Results_MLE.AUD_Variance)+(1/Results_MLE.VIS_Variance));
Results_MLE.VIS_Westimate= (1/Results_MLE.VIS_Variance)/((1/Results_MLE.VIS_Variance)+(1/Results_MLE.AUD_Variance));
Results_MLE.AV_EstimatedVariance=(Results_MLE.AUD_Variance*Results_MLE.VIS_Variance)/(Results_MLE.AUD_Variance+Results_MLE.VIS_Variance);
  

